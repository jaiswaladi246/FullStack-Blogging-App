pipeline {
    agent any
    
    
    tools {
        maven 'maven3'
    }
    stages {
        
        stage('Compile') {
            steps {
                sh "mvn compile"  // Corrected the spelling from "complile" to "compile"
            }
        }
        
        stage('Test') {
            steps {
                sh 'mvn test'
            }
        }
        
        stage('Package') {
            steps {
                sh 'mvn package'
            }
        }
    }
}
